<?xml version="1.0" encoding="utf-8"?>
<animated-selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:id="@+id/skip"
        android:drawable="@drawable/ic_skip_next"
        android:state_activated="true"/>
    <item
        android:id="@+id/close"
        android:drawable="@drawable/ic_close"/>
    <transition
        android:drawable="@drawable/avd_close_to_skip"
        android:fromId="@id/close"
        android:reversible="true"
        android:toId="@id/skip"/>
</animated-selector>