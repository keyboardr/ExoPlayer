<?xml version="1.0" encoding="utf-8"?>
<vector
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:width="18dp"
    android:height="18dp"
    android:tint="?attr/colorControlNormal"
    android:viewportHeight="18"
    android:viewportWidth="18"
    tools:height="64dp"
    tools:tint="?attr/colorControlActivated"
    tools:width="64dp">

    <group
        android:name="iconGroup"
        android:pivotX="9"
        android:pivotY="9"
        android:rotation="90"
        android:translateX="0.75">

        <path
            android:name="iconPath"
            android:fillColor="@android:color/white"
            android:pathData="@string/path_play"/>
    </group>
    <group
        android:name="arrow"
        android:pivotX="14"
        android:pivotY="14"
        android:scaleX=".3"
        android:scaleY=".3">
        <group>
            <clip-path
                android:name="arrow_mask"
                android:pathData="@string/mask_arrow_full"/>
            <path
                android:name="arrow_path"
                android:fillColor="@android:color/white"
                android:pathData="@string/path_arrow_continue"/>
        </group>
    </group>
</vector>